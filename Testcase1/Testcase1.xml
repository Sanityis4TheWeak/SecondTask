<Ui xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.blizzard.com/wow/ui/">
	<Script file="Libs\LibStub\LibStub.lua" />
	<Include file="Libs\CallbackHandler-1.0\CallbackHandler-1.0.xml" />
	<Include file="Libs\AceAddon-3.0\AceAddon-3.0.xml" />
	<Include file="Libs\AceConsole-3.0\AceConsole-3.0.xml" />
	<Include file="Libs\AceEvent-3.0\AceEvent-3.0.xml" />

	<Frame name="MainFrame" inherits="BackdropTemplate" parent="UIParent" hidden="true">
		<Size x="900" y="600"/>
		<Anchors>
		<Anchor point="CENTER"/>
		</Anchors>
		 <KeyValues><KeyValue key="backdropInfo" value="BACKDROP_DIALOG_32_32" keyType="string" type="global" /></KeyValues>
		<Layers>
			<Layer level="ARTWORK">
			
				<Texture parentKey="firstPicture" file="Interface/Addons/Testcase1/resources/placeholder"><Size x="240" y = "160"/><Anchors><Anchor point='LEFT' x="80"/></Anchors>
				<Scripts><OnMouseDown>Testcase1:IsSelected(self)</OnMouseDown><OnEnter>Testcase1:GenerateTooltips(self)</OnEnter></Scripts>
				</Texture>
				<Texture parentKey="secondPicture" file="Interface/Addons/Testcase1/resources/placeholder"><Size x="240" y = "160"/><Anchors><Anchor point='RIGHT' x="-80"/></Anchors>
				<Scripts><OnMouseDown>Testcase1:IsSelected(self)</OnMouseDown><OnEnter>Testcase1:GenerateTooltips(self)</OnEnter></Scripts>
				</Texture>
			</Layer>
		</Layers>

		<Frames>
		
			<EditBox parentKey="searchBox" inherits="InputBoxTemplate"><Size y="200" x="400"/><Anchors><Anchor y="-10" point="TOP"/></Anchors></EditBox>
			<Button name="$parentSearchButton" inherits="UIPanelButtonTemplate" text="Change"><Size x="80" y="30"/><Anchors><Anchor y="20" point="BOTTOM"/></Anchors>
			<Scripts><OnClick>Testcase1:InputText(self)</OnClick></Scripts>
			</Button>
			<Button name="$parentCloseButton" inherits="UIPanelButtonTemplate"><Size x="30" y="30"/><Anchors><Anchor point="TOPRIGHT"/></Anchors>
			<Scripts><OnClick>MainFrame:Hide()</OnClick></Scripts>
			</Button>

		</Frames>
	
	
	</Frame>

</Ui>
